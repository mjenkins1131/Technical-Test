<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Bill Simmons" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="418,316,0,0"/>
        <Label Content="Real Time with Bill" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="239,316,0,0" RenderTransformOrigin="0.396,3.894"/>
        <Label Content="BBC" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="107,315,0,0"/>
        <Label Content="Diane Rehm" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="14.123,12.788" Margin="589,323,0,0"/>
        <RadioButton x:Name="DianeTrack" Content="Track" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="589,355,0,0" Checked="Diane_RadioButton_Checked" AutomationProperties.Name="DianeTrack"/>
        <RadioButton x:Name="BillSimTrack" Content="Track" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="418,352,0,0" Checked="BillSim_RadioButton_Checked" AutomationProperties.Name="BillSimTrack"/>
        <RadioButton x:Name="RealTimeTrack" Content="Track" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="239,349,0,0" Checked="RealTime_RadioButton_Checked" AutomationProperties.Name="RealTimeTrack"/>
        <RadioButton x:Name="BBCTrack" Content="Track" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="107,346,0,0" Checked="BBC_RadioButton_Checked" AutomationProperties.Name="BBCTrack"/>
        <Calendar x:Name="DateRange" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="261,84,0,0" SelectionMode="SingleRange" AutomationProperties.Name="DateRange"/>
        <Label Content="Select a date range and feed you want to track" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="218,28,0,0"/>
        <Button Content="Run" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="309,273,0,0" Click="Button_Click"/>
    </Grid>
</Window>
